apiready = function() {
	api.closeFrame({
		name : 'lingganHeadr'
	});
	api.closeFrame({
		name : 'linggan'
	});
	api.closeFrame({
		name : 'shejiao'
	});
	api.closeFrame({
		name : 'wode'
	});
	var nav = $api.byId('nav2');

	var nav_info = $api.offset(nav);
	api.openFrame({

		name : 'fangan',
		url : './html/fangan.html',
		rect : {
			x : 0,
			y : 0,
			w : api.winWidth,
			h : api.winHeight - nav_info.h - 22
		}
	});

}
